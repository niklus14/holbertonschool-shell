#!/usr/bin/env bash

# Iterate over the files in the current directory (excluding hidden ones)
for file in *; do
    # Check if it's a regular file (optional check for files or directories)
    if [[ -f "$file" || -d "$file" ]]; then
        # Use 'cut' to extract the part after the first dash
        echo "$file" | cut -d'-' -f2-
    fi
done
